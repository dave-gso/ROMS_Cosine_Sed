   akox == 30.0d0
   
! Light attenuation coefficient of water [1/m]    

   ak1 == 0.046d0

! Specific light attenuation coefficient for      
!              phytoplankton [1/m/(mmol_N/m3)].              

   ak2 == 0.030d0

! Mesozooplankton specific mortality rate [1/day].      

   bgamma0 == 0.035d0

